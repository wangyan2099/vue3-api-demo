    "gsap": "^3.12.2",
    "howler": "^2.2.4",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.0",
    "sortablejs": "^1.15.0",
    "vditor": "^3.9.7",
    "video.js": "^8.6.1",
    "vue": "^3.3.8",
    "vue-echarts": "^6.6.5",
    "vue-i18n": "^9.6.5",
    "vue-router": "^4.2.5"
  },
